{
  "extends": "astro/tsconfigs/strict",
  "compilerOptions": {
    "allowJs": true,
    "checkJs": false,
    "baseUrl": ".",
    "paths": {
      "@/*": [
        "./src/*"
      ],
      "~/*": [
        "./public/*"
      ]
    },
    "jsx": "react-jsx",
    "jsxImportSource": "react",
    "lib": [
      "es2016",
      "dom"
    ]
  },
  "exclude": [
    "dist"
  ],
}